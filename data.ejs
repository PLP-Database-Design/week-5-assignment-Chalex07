    <table class="table table-bordered">
      <thead>
        <tr>
          <% if (title === 'Patient List') { %>
            <th>Patient ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Date of Birth</th>
          <% } else if (title === 'Provider List') { %>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Specialty</th>
          <% } %>
        </tr>
      </thead>
      <tbody>
        <% records.forEach(record => { %>
          <tr>
            <% if (title === 'Patient List') { %>
              <td><%= record.patient_id %></td>
              <td><%= record.first_name %></td>
              <td><%= record.last_name %></td>
              <td><%= record.date_of_birth %></td>
            <% } else if (title === 'Provider List') { %>
              <td><%= record.first_name %></td>
              <td><%= record.last_name %></td>
              <td><%= record.provider_specialty %></td>
            <% } %>
          </tr>
        <% }); %>
      </tbody>
    </table>
